<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.appforbraillemodule"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="15" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@android:style/Theme.NoTitleBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.appforbraillemodule.UpAndDown"
            android:theme="@android:style/Theme.NoTitleBar" >
        </activity>
        <activity android:name="com.example.appforbraillemodule.FileChooser" >
        </activity>
        <activity android:name="com.example.appforbraillemodule.ReadTextFile" >
        </activity>
        <activity
            android:name="com.example.appforbraillemodule.TextActivity"
            android:theme="@android:style/Theme.NoTitleBar" >
        </activity>
        <activity android:name="com.example.appforbraillemodule.BT_Preference" >
        </activity>
    </application>

</manifest>